RewriteEngine On
RewriteBase /
RewriteRule ^space/(.+)\/$ space\.php\?user=$1
RewriteRule ^store-([0-9]+)\/$ store\.php\?uid=$1
RewriteRule ^store-([0-9]+)/([^\/]+).html$ store\.php\?uid=$1&Uid=$2
RewriteCond %{REQUEST_FILENAME} !-d

